// Package dtypes exports functions for dtypes supported by GX.
package dtypes

func Bool(x bool) bool {
	return !x
}

func Float32(x float32) float32 {
	return -x
}

func Float64(x float64) float64 {
	return -x
}

func Int32(x int32) int32 {
	return -x
}

func Int64(x int64) int64 {
	return -x
}

func Uint32(x uint32) uint32 {
	return -x
}

func Uint64(x uint64) uint64 {
	return -x
}

func ArrayBool(x [2][3]bool) [2][3]bool {
	return !x
}

func ArrayFloat32(x [2][3]float32) [2][3]float32 {
	return -x
}

func ArrayFloat64(x [2][3]float64) [2][3]float64 {
	return -x
}

func ArrayInt32(x [2][3]int32) [2][3]int32 {
	return -x
}

func ArrayInt64(x [2][3]int64) [2][3]int64 {
	return -x
}

func ArrayUint32(x [2][3]uint32) [2][3]uint32 {
	return -x
}

func ArrayUint64(x [2][3]uint64) [2][3]uint64 {
	return -x
}

