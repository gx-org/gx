func TestMulTensor3DFloat64() [2][2][2]float64 {
	a := [2][2][2]float64{
		{
			{1, 2},
			{3, 4},
		},
		{
			{5, 6},
			{7, 8},
		},
	}
	b := [2][2][2]float64{
		{
			{8, 7},
			{6, 5},
		},
		{
			{4, 3},
			{2, 1},
		},
	}
	return a * b
	// Want:
	// [2][2][2]float64{
	// 	{
	// 		{8, 14},
	// 		{18, 20},
	// 	},
	// 	{
	// 		{20, 18},
	// 		{14, 8},
	// 	},
	// }
}

