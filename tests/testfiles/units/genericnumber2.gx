package genericnumber2

type (
	ints interface {
		int32 | int64
	}

	floats interface {
		float32 | float64
	}
)

func addTwo[T ints, U floats](x [___S]T, y [S___]U) ([S___]T, [S___]U) {
	return x + 5, y + 7
}

func TestAddTwo() ([2]int32, [2]float64) {
	return addTwo([2]int32{1, 2}, [2]float64{3.0, 4})
	// Want:
	// 0: [2]int32{6, 7}
	// 1: [2]float64{10, 11}
