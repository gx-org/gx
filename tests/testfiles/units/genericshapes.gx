package genericshapes

import (
	"dtype"
	"num"
)

func traceGenericShapes[T dtype.Num](a [_A][_B]T, b [___S]T) [A][B]int64 {
	trace(A)
	trace(B)
	trace(S)
	return num.IotaFull([]intlen{A, B})
}

func TestTraceGenericShapes() [2][3]int64 {
	return traceGenericShapes(
		[2][3]float32{},
		[4][5][6]float32{},
	)
	// Want:
	// [A][B]int64{
	// 	{0, 1, 2},
	// 	{3, 4, 5},
	// }
	// Trace:
	// genericshapes.gx:0
	// 	intlen(2)
	// genericshapes.gx:1
	// 	intlen(3)
	// genericshapes.gx:2
	// 	[intlen(4),intlen(5),intlen(6)]
}

