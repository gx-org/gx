package trace_test

func TestTraceScalar() float64 {
	a := 2
	trace(a)
	a = a + 1
	trace(a)
	return a
	// Want:
	// float64(3)
	// Trace:
	// trace_test.gx:0
	//   float64(2)
	// trace_test.gx:1
	//   float64(3)
}

func TestTrace() [2]float64 {
	a := [2]float64{} + 2
	trace(a)
	a = a + 1
	trace(a)
	return a
	// Want:
	// [2]float64{3, 3}
	// Trace:
	// trace_test.gx:0
	//   [2]float64{2, 2}
	// trace_test.gx:1
	//   [2]float64{3, 3}
}
